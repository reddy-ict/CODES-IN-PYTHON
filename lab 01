# Program to display “Welcome to Python” five times

for i in range(5):
    print("Welcome to Python")


# Program to display the following table

print("a   a^2   a^3")
for a in range(1, 5):
    print(a, "  ", a**2, "   ", a**3)


# Program to display the result of a mathematical expression

result = (9.5 * 4.5 - 2.5 * 3) / (45.5 - 3.5)
print("Result is:", result)
